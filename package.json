{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc server/index.ts --outDir dist --moduleResolution node --target es2020 --module commonjs || tsx build server/index.ts --outDir=dist",
    "start": "node dist/server/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:security": "jest --testPathPattern=security",
    "test:integration": "jest --testPathPattern=integration",
    "validate:env": "node -e \"require('./server/services/environment-validator.ts').environmentValidator.validateEnvironment()\"",
    "prepare:deployment": "chmod +x prepare-deployment.sh && ./prepare-deployment.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "generate-keys": "node -e \"const crypto = require('crypto'); console.log('JWT_SECRET=' + crypto.randomBytes(64).toString('hex')); console.log('SESSION_SECRET=' + crypto.randomBytes(32).toString('hex')); console.log('ENCRYPTION_KEY=' + crypto.randomBytes(32).toString('hex'));\"",
    "health-check": "curl -f http://0.0.0.0:5000/api/health || exit 1"
  },
