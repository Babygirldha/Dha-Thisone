# Application Environment
NODE_ENV=production
PORT=5000

# Core Security Keys (Generate with: npm run generate-keys)
JWT_SECRET=your_64_character_jwt_secret_here
SESSION_SECRET=your_32_character_session_secret_here
ENCRYPTION_KEY=your_32_character_encryption_key_here
VITE_ENCRYPTION_KEY=your_32_character_vite_encryption_key_here

# Advanced Security Keys
MASTER_ENCRYPTION_KEY=your_64_character_master_encryption_key_here
QUANTUM_ENCRYPTION_KEY=your_64_character_quantum_encryption_key_here
BIOMETRIC_ENCRYPTION_KEY=your_32_character_biometric_key_here
DOCUMENT_SIGNING_KEY=your_32_character_document_signing_key_here

# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:5432/database?sslmode=require

# AI Services (Ultra Queen AI - Multiple Providers)
