[[workflows.workflow]]
name = "Emergency Deploy"
author = 47957021
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üÜò EMERGENCY DEPLOYMENT\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rm -rf node_modules dist"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install --force --legacy-peer-deps"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "mkdir -p dist/public"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cp -r client/index.html dist/public/"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd client"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "vite build --config vite.config.ts"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd .."

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "tsc -p tsconfig.server.json"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"

