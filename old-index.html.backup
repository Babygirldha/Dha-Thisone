            font-weight: bold;
            margin-top: 10px;
            font-size: 14px;
        }
        .person-section {
            background: #f8f9fa;
            padding: 20px;
            border-radius: 10px;
            margin-bottom: 25px;
            border-left: 5px solid #005a3c;
        }
        .person-section h2 {
            color: #005a3c;
            margin-bottom: 15px;
            font-size: 20px;
        }
        .doc-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 15px;
            margin-top: 15px;
        }
        .doc-item {
            background: white;
            border: 2px solid #e0e0e0;
            border-radius: 8px;
            padding: 15px;
            transition: all 0.3s;
            cursor: pointer;
        }
        .doc-item:hover {
            border-color: #005a3c;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(0, 90, 60, 0.2);
        }
        .doc-item:active {
            transform: scale(0.98);
        }
        .doc-icon {
            font-size: 30px;
            margin-bottom: 8px;
        }
        .doc-title {
            font-weight: bold;
            color: #333;
            font-size: 14px;
            margin-bottom: 5px;
        }
        .doc-details {
            font-size: 12px;
            color: #666;
            line-height: 1.4;
        }
        .doc-status {
            display: inline-block;
            background: #28a745;
            color: white;
            padding: 2px 8px;
            border-radius: 4px;
            font-size: 10px;
            margin-top: 5px;
        }
        .generate-all-btn {
            width: 100%;
            background: linear-gradient(135deg, #005a3c, #007850);
            color: white;
            border: none;
            padding: 20px;
            border-radius: 10px;
            font-size: 18px;
            font-weight: bold;
            cursor: pointer;
            transition: all 0.3s;
            margin-top: 30px;
        }
        .generate-all-btn:hover {
            transform: translateY(-2px);
            box-shadow: 0 10px 30px rgba(0, 90, 60, 0.4);
        }
        .generate-all-btn:disabled {
            opacity: 0.7;
            cursor: wait;
        }
        .success-msg {
            background: #d4edda;
            color: #155724;
            padding: 15px;
            border-radius: 10px;
            margin-top: 20px;
            display: none;
            text-align: center;
            border: 1px solid #c3e6cb;
        }
        .security-badge {
            position: fixed;
            top: 20px;
            right: 20px;
            background: rgba(0, 0, 0, 0.8);
            color: #4caf50;
            padding: 10px 20px;
            border-radius: 30px;
            font-size: 12px;
            display: flex;
            align-items: center;
            gap: 8px;
            z-index: 1000;
        }
        .doc-count {
            background: #005a3c;
            color: white;
            padding: 10px 20px;
            border-radius: 10px;
            text-align: center;
            margin-bottom: 20px;
            font-weight: bold;
        }
        .progress-bar {
            width: 100%;
            height: 8px;
            background: #e0e0e0;
            border-radius: 4px;
            overflow: hidden;
            margin-top: 20px;
            display: none;
        }
        .progress-fill {
            height: 100%;
            background: linear-gradient(90deg, #005a3c, #ffd700);
            width: 0%;
            transition: width 0.3s ease;
        }
        .toast {
            position: fixed;
            bottom: 30px;
            right: 30px;
            background: #333;
            color: white;
            padding: 15px 25px;
            border-radius: 8px;
            display: none;
            z-index: 2000;
            animation: slideUp 0.3s ease;
        }
        @keyframes slideUp {
            from { transform: translateY(100px); opacity: 0; }
            to { transform: translateY(0); opacity: 1; }
        }
    </style>
</head>
