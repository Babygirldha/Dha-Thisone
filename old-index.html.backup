    <script>
        const { jsPDF } = window.jspdf;
        
        let documentsGenerated = 0;
        const totalDocuments = 11;
        
        function showToast(message) {
            const toast = document.getElementById('toast');
            toast.textContent = message;
            toast.style.display = 'block';
            setTimeout(() => {
                toast.style.display = 'none';
            }, 3000);
        }
        
        function updateProgress() {
            documentsGenerated++;
            const percentage = (documentsGenerated / totalDocuments) * 100;
            document.getElementById('progressFill').style.width = percentage + '%';
            if (documentsGenerated === totalDocuments) {
                setTimeout(() => {
                    document.getElementById('successMsg').style.display = 'block';
                    document.getElementById('progressBar').style.display = 'none';
                    documentsGenerated = 0;
                }, 500);
            }
        }
        
        function generateVerificationCode() {
            const timestamp = Date.now();
            const random = Math.floor(Math.random() * 999999).toString().padStart(6, '0');
            return `DHA${timestamp}${random}`;
        }
        
        function generateDocNumber(prefix) {
            const date = new Date();
            const year = date.getFullYear();
            const month = String(date.getMonth() + 1).padStart(2, '0');
            const day = String(date.getDate()).padStart(2, '0');
            const random = Math.floor(Math.random() * 100000).toString().padStart(5, '0');
            return `${prefix}/${year}/${month}/ZA${random}`;
        }
        
        function addDHAHeader(doc, title, docNum) {
            // Republic of South Africa Header
            doc.setFontSize(10);
            doc.text('REPUBLIC OF SOUTH AFRICA', 105, 20, { align: 'center' });
            doc.setFontSize(12);
            doc.text('DEPARTMENT OF HOME AFFAIRS', 105, 27, { align: 'center' });
            doc.setFontSize(16);
            doc.setFont(undefined, 'bold');
            doc.text(title, 105, 40, { align: 'center' });
            doc.setFont(undefined, 'normal');
            
            doc.setFontSize(10);
            doc.text(`Document No: ${docNum}`, 190, 50, { align: 'right' });
            
            // Decorative line
            doc.setDrawColor(0, 90, 60);
            doc.setLineWidth(1);
            doc.line(20, 45, 190, 45);
            
            // Official stamp
            doc.setDrawColor(255, 0, 0);
            doc.setLineWidth(2);
            doc.rect(150, 10, 45, 15);
            doc.setTextColor(255, 0, 0);
            doc.setFontSize(10);
            doc.text('OFFICIAL', 172.5, 18, { align: 'center' });
            doc.setTextColor(0, 0, 0);
        }
        
        function addDHAFooter(doc, verificationCode) {
            // QR Code Placeholder
            doc.setDrawColor(0, 0, 0);
            doc.setLineWidth(1);
            doc.rect(20, 250, 30, 30);
            doc.setFontSize(6);
            doc.text('QR CODE', 35, 265, { align: 'center' });
            doc.text('SCAN TO', 35, 270, { align: 'center' });
            doc.text('VERIFY', 35, 275, { align: 'center' });
            
            // Verification Information
            doc.setFontSize(8);
            doc.text(`Verification Code: ${verificationCode}`, 60, 260);
            doc.text(`Online Verification: https://dha.gov.za/verify/${verificationCode}`, 60, 265);
            doc.text('Call Centre: 0800 60 11 90', 60, 270);
            doc.text('Email: info@dha.gov.za', 60, 275);
            
            // Legal Footer
            doc.setFontSize(8);
            doc.text('This is an official document issued by the Department of Home Affairs', 105, 285, { align: 'center' });
            doc.text('Immigration Act, 2002 (Act No. 13 of 2002)', 105, 290, { align: 'center' });
            doc.text('Any unauthorized alteration renders this document invalid', 105, 295, { align: 'center' });
        }
        
        function addOfficialSeal(doc, x, y) {
            doc.setDrawColor(0, 90, 60);
            doc.setLineWidth(2);
            doc.circle(x, y, 15);
            doc.setFontSize(10);
            doc.text('DHA', x, y - 3, { align: 'center' });
            doc.text('OFFICIAL', x, y + 2, { align: 'center' });
            doc.text('SEAL', x, y + 7, { align: 'center' });
        }
        
