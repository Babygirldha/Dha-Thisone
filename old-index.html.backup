        .flag {
            font-size: 70px;
            margin-bottom: 10px;
            animation: pulse 2s infinite;
        }
        @keyframes pulse {
            0%, 100% { transform: scale(1); }
            50% { transform: scale(1.05); }
        }
        h1 {
            color: #005a3c;
            font-size: 28px;
            margin-bottom: 10px;
        }
        .subtitle {
